{
  "name": "@joyautomation/synapse",
  "version": "0.0.42",
  "description": "Synapse is a tool for creating and managing neural networks.",
  "author": "Joy Automation",
  "license": "GPL-3.0",
  "repository": "github:joyautomation/synapse",
  "homepage": "https://github.com/joyautomation/synapse#readme",
  "bugs": "https://github.com/joyautomation/synapse/issues",
  "declaration": true,
  "exports": "./index.ts",
  "tasks": {
    "example:host": "deno run --env-file=.env --allow-env --allow-net --allow-read --allow-write --watch examples/host.ts ",
    "example:node": "deno run --env-file=.env --allow-env --allow-net --allow-read --allow-write --watch examples/node.ts ",
    "test": "deno test --allow-env",
    "test:watch": "deno test --allow-env --watch"
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "@joyautomation/coral": "jsr:@joyautomation/coral@^0.0.12",
    "@m3o/version": "jsr:@m3o/version@^0.1.11",
    "@std/assert": "jsr:@std/assert@1",
    "@std/expect": "jsr:@std/expect@^1.0.2",
    "@std/testing": "jsr:@std/testing@^1.0.2",
    "date-fns": "npm:date-fns@^3.6.0",
    "long": "npm:long@^5.2.3",
    "mqtt": "npm:mqtt@^5.10.1",
    "pako": "npm:pako@^2.1.0",
    "ramda": "npm:ramda@^0.30.1",
    "sparkplug-payload": "npm:sparkplug-payload@1.0.3"
  }
}